<UserControl x:Class="Apache_Manager.Controls.module.mainmodules"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             mc:Ignorable="d" 
             d:DesignHeight="612" d:DesignWidth="890">
    <Grid>
        <Button x:Name="addmodule" Content="Dodaj moduł" Style="{StaticResource MetroButton}" HorizontalAlignment="Left" Margin="45,25,0,0" VerticalAlignment="Top" Click="addmodule_Click" Width="220"/>
        <Button x:Name="deletemodule" Content="Usuń moduł" Style="{StaticResource MetroButton}" HorizontalAlignment="Left" Margin="335,25,0,0" VerticalAlignment="Top" Click="deletemodule_Click" Width="220"/>
        <Button x:Name="activemodule" Content="Włącz moduł" Style="{StaticResource MetroButton}" HorizontalAlignment="Left" Margin="625,25,0,0" VerticalAlignment="Top"  Click="activemodule_Click" Width="220"/>
        <Button x:Name="deactiveaddmodule" Content="Wyłącz moduł" Style="{StaticResource MetroButton}" HorizontalAlignment="Left" Margin="45,225,0,0" VerticalAlignment="Top" Click="deactiveaddmodule_Click" Width="220"/>

    </Grid>
</UserControl>
