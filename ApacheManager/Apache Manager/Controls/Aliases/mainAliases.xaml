<UserControl x:Class="Apache_Manager.Controls.Aliases.mainAliases"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             mc:Ignorable="d" 
             d:DesignHeight="612" d:DesignWidth="890">
    <Grid>
        <Button x:Name="createaliases" Content="Dodaj moduł" Style="{StaticResource MetroButton}" HorizontalAlignment="Left" Margin="45,25,0,0" VerticalAlignment="Top" Click="createaliases_Click" Width="220"/>
        <Button x:Name="deletealiases" Content="Usuń moduł" Style="{StaticResource MetroButton}" HorizontalAlignment="Left" Margin="335,25,0,0" VerticalAlignment="Top" Click="deletealiases_Click" Width="220"/>
        <Button x:Name="editaliases" Content="Włącz moduł" Style="{StaticResource MetroButton}" HorizontalAlignment="Left" Margin="625,25,0,0" VerticalAlignment="Top"  Click="editaliases_Click" Width="220"/>

    </Grid>
</UserControl>
